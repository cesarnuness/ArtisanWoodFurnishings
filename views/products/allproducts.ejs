<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>All Products</h1>
    <p>List of all products will be here</p>

    <% for (const product of products) { %>
    <div>
      <h3><%= `ID: ${product.id} NAME: ${product.name}` %></h3>
    </div>
    <% } %>
  </body>
</html>
