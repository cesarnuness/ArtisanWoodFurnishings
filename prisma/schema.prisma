datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  products Product[] // Define the relationship with products
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  categories String[]
  quantity  Int
  creator   User     @relation(fields: [creatorId], references: [id])
  creatorId Int
}